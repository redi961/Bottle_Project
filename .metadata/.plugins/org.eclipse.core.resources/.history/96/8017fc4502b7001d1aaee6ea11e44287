package edu.pnu.domain;

public class BottleUser {
	private String user_id;	// email
	private String user_pass;
	private String user_phon_num;
	private String user_name;
	
	public BottleUser() {
		// TODO Auto-generated constructor stub
	}
	
	public BottleUser(String user_id, String user_pass, String user_phon_num, String user_name) {
		super();
		this.user_id = user_id;
		this.user_pass = user_pass;
		this.user_phon_num = user_phon_num;
		this.user_name = user_name;
	}

	@Override
	public String toString() {
		return "BottleUser [user_id=" + user_id + ", user_pass=" + user_pass + ", user_phon_num=" + user_phon_num
				+ ", user_name=" + user_name + "]";
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public String getUser_pass() {
		return user_pass;
	}

	public void setUser_pass(String user_pass) {
		this.user_pass = user_pass;
	}

	public String getUser_phon_num() {
		return user_phon_num;
	}

	public void setUser_phon_num(String user_phon_num) {
		this.user_phon_num = user_phon_num;
	}

	public String getUser_name() {
		return user_name;
	}

	public void setUser_name(String user_name) {
		this.user_name = user_name;
	}
	
}
